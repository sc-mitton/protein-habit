{
  "cli": {
    "version": ">=10.2.4",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_VARIANT": "production"
      },
      "channel": "production"
    },
    "development:simulator": {
      "developmentClient": true,
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "development:preview": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "APP_ENV": "preview"
      },
      "environment": "preview"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "preview"
      },
      "channel": "preview"
    }
  },
  "submit": {
    "production": {}
  }
}
