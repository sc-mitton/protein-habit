{
  "cli": {
    "version": ">=10.2.4",
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "development:simulator": {
      "developmentClient": true,
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
