{
  "cli": {
    "version": ">=10.2.4",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production",
        "APP_VARIANT": "production",
        "NODE_ENV": "production"
      },
      "channel": "production"
    },
    "development:preview": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "preview",
        "APP_VARIANT": "preview",
        "NODE_ENV": "development"
      },
      "environment": "preview"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "preview",
        "APP_VARIANT": "preview",
        "NODE_ENV": "production"
      },
      "channel": "preview"
    }
  },
  "submit": {
    "production": {}
  }
}
